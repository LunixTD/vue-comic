// tab点击动画
.touchStart
  animation: touchStart .1s cubic-bezier(0,.88,1,.98) 1
  animation-fill-mode: forwards
  @keyframes touchStart
    0%
      transform: scale(1)
      background-color: ''
    100%
      transform: scale(0.96)
      background-color: rgba(0,0,0,0.5)
.touchEnd
  animation: touchEnd .15s ease-out
  animation-fill-mode: forwards
  @keyframes touchEnd
    0%
      transform: scale(0.96)
      background-color: rgba(0,0,0,0.5)
    100%
      transform: scale(1)
      background-color: ''

